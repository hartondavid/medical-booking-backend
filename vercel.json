{
    "version": 2,
    "builds": [
        {
            "src": "index.mjs",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/public/(.*)",
            "destination": "/public/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.mjs"
        }
    ],
    "functions": {
        "index.mjs": {
            "maxDuration": 30
        }
    }
}